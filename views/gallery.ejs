  <hr class="veil-md offset-top-10" />
  <section
    class="section-md-122 section-title section-title-left section-title-left-2"
    data-title="gallery"
  >
    <div class="shell shell-wide">
      <h4 class="offset-top-0 offset-md-top-65">Our NFTs</h4>
      <div class="row">
        <!-- Isotope Filters-->
        <!-- <div class="col-lg-12">
            <div class="isotope-filters isotope-filters-horizontal">
              <ul class="list-inline">
                <li><a class="active" data-isotope-filter="*" data-isotope-group="gallery" href="#">show all</a></li>
                <li><a data-isotope-filter="Category 1" data-isotope-group="gallery" href="#">first category</a></li>
                <li><a data-isotope-filter="Category 2" data-isotope-group="gallery" href="#">second category</a></li>
                <li><a data-isotope-filter="Category 3" data-isotope-group="gallery" href="#">third category</a></li>
              </ul>
            </div>
          </div> -->
        <!-- PhotoSwipe-->
        <!-- Isotope Content-->
        <div class="col-lg-12" data-photo-swipe-gallery="gallery">
          <div
            class="isotope"
            data-isotope-layout="fitRows"
            data-isotope-group="gallery"
          >
            <div class="row">
              <% for(var i = 0;i < dogs.length;i++){ %>
              <div class="col-xs-12 col-sm-6 col-md-4 isotope-item thumbnail">
                <img
                  class="img-responsive"
                  width="520"
                  height="391"
                  src="<%= dogs[i].nftpicture %>"
                  alt=""
                />
                <div class="caption">
                  <h4 class="text-uppercase">
                    <a
                      style="color: #313131"
                      href="/nftDetails/<%= dogs[i].id %>"
                      >Name:<%= dogs[i].name %></a
                    >
                  </h4>
                  <p class="small text-bold">
                    Story:<% if(dogs[i].story.length<100){%> <%= dogs[i].story
                    %> <% }else{ %> <%= dogs[i].story.substring(0,100) %> ....
                    <% } %>
                  </p>
                  <div class="tags-custom">
                    <% if(dogs[i].vaccinated){ %>
                    <span><small class="flex-items">Vaccinated</small></span>
                    <% }if(dogs[i].neutered){ %>
                    <span><small class="flex-items">Neutered</small></span>
                    <% }if(dogs[i].spayed){ %>
                    <span><small class="flex-items">Spayed</small></span>
                    <% } %> <% if(dogs[i].sex){ %>
                    <span><small class="flex-items">Male</small></span>
                    <% }else{ %>
                    <span><small class="flex-items">Female</small></span>
                    <% } %>
                  </div>

                  <a
                    class="caption-link btn btn-default"
                    href="<%= dogs[i].nftlink %>"
                    >Buy</a
                  >
                </div>
              </div>
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</main>

